<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Note</th>
      <th></th>
    </tr>
  </thead>
  <tbody data-link="row" class="rowlink">
    <% exam.patient_diags.each do |pd| %> 
    <tr>
      <td><%= pd.id %></td>
      <td><%= pd.diag.name %></td>
      <td><%= pd.note %></td>
      <td class="rowlink-skip"><a href="<%= edit_patient_diag_path(pd) %>" data-remote="true" data-disable-with='<span class="glyphicon glyphicon-time"></span>'><span class="glyphicon glyphicon-edit"></span></a></td>
    </tr>
    <% end %>
  </tbody>
</table>