<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>id</th>
      <th>diag name</th>
      <th>note</th>
    </tr>
  </thead>
  <tbody>
    <% exam.exams_diags.each do |ed| %> 
    <tr>
      <th scope="row"><%= ed.id %></th>
      <td><%= ed.diag.name %></td>
      <td><%= ed.note %></td>
    </tr>
    <% end %>
  </tbody>
</table>